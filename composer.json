{
    "name": "flodaq/ticket-notification-bundle",
    "description": "This bundle provides e-mail notifications for HackzillaTicketBundle events.",
    "keywords": [
        "ticket",
        "ticketing",
        "help desk",
        "helpdesk",
        "support",
        "multilingual",
        "notification"
    ],
    "type": "symfony-bundle",
    "license": "MIT",
    "authors": [
        {
            "name": "Florent DAQUET",
            "email": "flodaq@linkthis.fr",
            "homepage": "http://linkthis.fr",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^5.6|^7.0",
        "hackzilla/ticket-bundle": "3.0.x-dev",
        "symfony/config": "^2.8|^3.0|^4.0",
        "symfony/dependency-injection": "^2.8|^3.0|^4.0",
        "symfony/event-dispatcher": "^2.8|^3.0|^4.0",
        "symfony/http-kernel": "^2.8|^3.0|^4.0",
        "symfony/swiftmailer-bundle": "^2.3.9|^3.0"
    },
    "require-dev": {
        "symfony/phpunit-bridge": "^3.4.19|^4.0"
    },
    "conflict": {
        "phpunit/phpunit": "<5.4.3",
        "swiftmailer/swiftmailer": "<5.1"
    },
    "autoload": {
        "psr-4": {
            "Flodaq\\TicketNotificationBundle\\": ""
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
